{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mddfDataFactory"
		},
		"mddf_ADLS_conn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mddf_ADLS_conn'"
		},
		"mddf_blob_conn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mddf_blob_conn'"
		},
		"mddf_hdinsight_conn_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'mddf_hdinsight_conn'"
		},
		"mddf_sanitycheck_conn_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'mddf_sanitycheck_conn'"
		},
		"AzureSynapseAnalytics_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics'"
		},
		"mddf_ADLS_conn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mddfadls.dfs.core.windows.net"
		},
		"mddf_hdinsight_conn_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"stage1_IndividualDetails_trggr_properties_stage1_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"stage1_IndividualDetails_trggr_properties_stage1_parameters_FilePath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"stage1_IndividualDetails_trggr_properties_stage1_parameters_ParamFilePath": {
			"type": "string",
			"defaultValue": "mddfroot/params"
		},
		"stage1_IndividualDetails_trggr_properties_stage1_parameters_ParamFileName": {
			"type": "string",
			"defaultValue": "individualdetails_params.csv"
		},
		"stage1_IndividualDetails_trggr_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/78e5420a-a2c2-4b90-a0fb-63f8606cc913/resourceGroups/POC/providers/Microsoft.Storage/storageAccounts/mddfadls"
		},
		"stage1_totalcase_trggr_properties_stage1_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"stage1_totalcase_trggr_properties_stage1_parameters_FilePath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"stage1_totalcase_trggr_properties_stage1_parameters_ParamFilePath": {
			"type": "string",
			"defaultValue": "mddfroot/params"
		},
		"stage1_totalcase_trggr_properties_stage1_parameters_ParamFileName": {
			"type": "string",
			"defaultValue": "totalcases_params.csv"
		},
		"stage1_totalcase_trggr_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/78e5420a-a2c2-4b90-a0fb-63f8606cc913/resourceGroups/POC/providers/Microsoft.Storage/storageAccounts/mddfadls"
		},
		"stage2_trggr_properties_stage2_parameters_ParamPath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"stage2_trggr_properties_stage2_parameters_ParamName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"stage2_trggr_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/78e5420a-a2c2-4b90-a0fb-63f8606cc913/resourceGroups/POC/providers/Microsoft.Storage/storageAccounts/mddfadls"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"mddf_ADLS_conn_accountKey": {
						"value": "[parameters('mddf_ADLS_conn_accountKey')]"
					},
					"mddf_blob_conn_connectionString": {
						"value": "[parameters('mddf_blob_conn_connectionString')]"
					},
					"mddf_sanitycheck_conn_functionKey": {
						"value": "[parameters('mddf_sanitycheck_conn_functionKey')]"
					},
					"AzureSynapseAnalytics_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics_connectionString')]"
					},
					"mddf_hdinsight_conn_password": {
						"value": "[parameters('mddf_hdinsight_conn_password')]"
					},
					"mddf_ADLS_conn_properties_typeProperties_url": {
						"value": "[parameters('mddf_ADLS_conn_properties_typeProperties_url')]"
					},
					"mddf_hdinsight_conn_properties_typeProperties_userName": {
						"value": "[parameters('mddf_hdinsight_conn_properties_typeProperties_userName')]"
					},
					"stage1_IndividualDetails_trggr_properties_stage1_parameters_FileName": {
						"value": "[parameters('stage1_IndividualDetails_trggr_properties_stage1_parameters_FileName')]"
					},
					"stage1_IndividualDetails_trggr_properties_stage1_parameters_FilePath": {
						"value": "[parameters('stage1_IndividualDetails_trggr_properties_stage1_parameters_FilePath')]"
					},
					"stage1_IndividualDetails_trggr_properties_stage1_parameters_ParamFilePath": {
						"value": "[parameters('stage1_IndividualDetails_trggr_properties_stage1_parameters_ParamFilePath')]"
					},
					"stage1_IndividualDetails_trggr_properties_stage1_parameters_ParamFileName": {
						"value": "[parameters('stage1_IndividualDetails_trggr_properties_stage1_parameters_ParamFileName')]"
					},
					"stage1_IndividualDetails_trggr_properties_typeProperties_scope": {
						"value": "[parameters('stage1_IndividualDetails_trggr_properties_typeProperties_scope')]"
					},
					"stage1_totalcase_trggr_properties_stage1_parameters_FileName": {
						"value": "[parameters('stage1_totalcase_trggr_properties_stage1_parameters_FileName')]"
					},
					"stage1_totalcase_trggr_properties_stage1_parameters_FilePath": {
						"value": "[parameters('stage1_totalcase_trggr_properties_stage1_parameters_FilePath')]"
					},
					"stage1_totalcase_trggr_properties_stage1_parameters_ParamFilePath": {
						"value": "[parameters('stage1_totalcase_trggr_properties_stage1_parameters_ParamFilePath')]"
					},
					"stage1_totalcase_trggr_properties_stage1_parameters_ParamFileName": {
						"value": "[parameters('stage1_totalcase_trggr_properties_stage1_parameters_ParamFileName')]"
					},
					"stage1_totalcase_trggr_properties_typeProperties_scope": {
						"value": "[parameters('stage1_totalcase_trggr_properties_typeProperties_scope')]"
					},
					"stage2_trggr_properties_stage2_parameters_ParamPath": {
						"value": "[parameters('stage2_trggr_properties_stage2_parameters_ParamPath')]"
					},
					"stage2_trggr_properties_stage2_parameters_ParamName": {
						"value": "[parameters('stage2_trggr_properties_stage2_parameters_ParamName')]"
					},
					"stage2_trggr_properties_typeProperties_scope": {
						"value": "[parameters('stage2_trggr_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}