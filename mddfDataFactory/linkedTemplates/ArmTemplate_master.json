{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mddfDataFactory"
		},
		"mddf_ADLS_conn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mddf_ADLS_conn'"
		},
		"mddf_hdinsight_conn_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'mddf_hdinsight_conn'"
		},
		"mddf_sanitycheck_conn_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'mddf_sanitycheck_conn'"
		},
		"mddf_ADLS_conn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mddfadls.dfs.core.windows.net"
		},
		"mddf_hdinsight_conn_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"mddf_ADLS_conn_accountKey": {
						"value": "[parameters('mddf_ADLS_conn_accountKey')]"
					},
					"mddf_sanitycheck_conn_functionKey": {
						"value": "[parameters('mddf_sanitycheck_conn_functionKey')]"
					},
					"mddf_hdinsight_conn_password": {
						"value": "[parameters('mddf_hdinsight_conn_password')]"
					},
					"mddf_ADLS_conn_properties_typeProperties_url": {
						"value": "[parameters('mddf_ADLS_conn_properties_typeProperties_url')]"
					},
					"mddf_hdinsight_conn_properties_typeProperties_userName": {
						"value": "[parameters('mddf_hdinsight_conn_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}